{
  "name": "pago-insibs",
  "version": "1.0.0",
  "description": "Plataforma de pagos digital con cuenta bancaria",
  "main": "server.js",
  "scripts": {
    "start": "env TZ=America/Caracas PORT=5000 NODE_ENV=production node server.js",
    "server": "env TZ=America/Caracas PORT=5000 nodemon server",
    "client": "env TZ=America/Caracas npm start --prefix ../client",
    "admin": "env TZ=America/Caracas npm start --prefix ../admin",
    "client-dev": "env TZ=America/Caracas concurrently \"npm run server\" \"npm run client\"",
    "admin-dev": "env TZ=America/Caracas concurrently \"npm run server\" \"npm run admin\""
  },
  "author": "Roger Rengifo",
  "license": "ISC",
  "dependencies": {
    "acorn": "^7.4.1",
    "aws-sdk": "^2.797.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "exceljs": "^3.9.0",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "generate-rsa-keypair": "^0.2.1",
    "google-auth-library": "^7.11.0",
    "helmet": "^3.22.0",
    "instapago": "^0.6.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nanoid": "^3.2.0",
    "open": "^6.4.0",
    "pdfkit": "^0.11.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pug": "^3.0.2",
    "read-excel-file": "^5.2.26",
    "redux-saga": "^1.1.3",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^6.2.0",
    "translate": "^1.1.2",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
